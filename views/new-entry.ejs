<% include header %>

<h2>식단 새롭게 작성</h2>

<form method="post" role="form">
  <div class="form-group">
    <label for="title">제목</label>
    <input type="text" class="form-control" id="title" name="title" placeholder="식단명 입력" required>
  </div>
  <div class="form-group">
    <label for="body">내용을 입력하세요.</label>
    <textarea class="form-control" id="body" name="body" placeholder="먹고 싶은 음식을 먹는 것은 행복한 일이에요. 한번 먹고 싶은 것을 말해볼까용?" rows="3" required></textarea>
  </div>
  <div class="form-group">
    <input type="submit" value="식단 등록" class="btn btn-primary">
  </div>
</form>

<% include footer %>
